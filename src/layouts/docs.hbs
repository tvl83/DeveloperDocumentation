---
date: <%= new Date() %>
---
<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">

    <title>Sphero Docs | {{title}}</title>

    <!-- jQuery -->
    <script src="{{assets}}/js/jquery-1.11.1.min.js"></script>

    <!-- Styles-->
    <link href="{{assets}}/css/style.css" rel="stylesheet">

    <!-- Let me embed vines -->
    <script async src="//platform.vine.co/static/scripts/embed.js" charset="utf-8"></script>

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{assets}}/sphero.ico" type="image/x-icon" />

    <!-- Vendor theme mods -->
    <script src="{{assets}}/js/jquery-styling.js"></script>
    <script src="{{assets}}/js/prettify.js"></script>

    <!-- Sphero theme mods -->

    <!-- Magnific Popup CSS and JS files -->
    <link rel="stylesheet" href="{{assets}}/css/magnific-popup.css">
    <script src="{{assets}}/js/jquery.magnific-popup.min.js"></script>

    <!-- Meta -->
    <meta content="Sphero" property="og:title">
    <meta content="Documentation for Sphero" name="description">

  </head>
  <body role="flatdoc" class="no-literate">
    <div id="site-wrapper">
      <div id="site-canvas">
        {{> mobile-nav}}
        <div id="docs">
          <header class="large-12 columns navigation always-show">
            <div class="left-side">
              <a href="http://www.sphero.com"><img src="{{assets}}/images/spheromark.png" id="logo"></a>
            </div>
            <div class="right-side">
              <div class="home-button">
                <div class="ion-ios7-cart-outline"></div>
                <a href="http://store.gosphero.com">Shop</a>
              </div>
              <div class="home-button">
                <div class="ion-arrow-left-c"></div>
                <a href="http://www.sphero.com">sphero.com</a>
              </div>
            </div>
          </header>
          <div class="content-root" data-spy="scroll" data-target="#toc">
            <div class="menubar">
              <div class="menu section" role="flatdoc-menu">
                {{> site-nav}}
              </div>
            </div>
            <div role="flatdoc-content" class="content" id="content">
              {{#is section "SDK Documentation"}}
                <ul class="language-picker">
                  <li class="language"><a href="#objective-c" data-language="objective-c" class="button">Objective-C</a></li>
                  <li class="language"><a href="#swift" data-language="swift" class="button">Swift</a></li>
                  <li class="language"><a href="#java" data-language="java" class="button">Android</a></li>
                  <li class="language"><a href="#js" data-language="unity" class="button">Unity</a></li>
                </ul>
              {{/is}}
              <h1>{{title}}
                <a href="https://github.com/orbotix/DeveloperDocumentation/edit/master/src/content/{{slugify section}}/{{slugify title}}.md" class="edit-link"><span class="ion-edit"></span></a>
              </h1>
              {{#markdown}}
              {{> body}}
              {{/markdown}}
            </div>
          </div>
        </div>
        {{> footer}}
      </div>
    </div>

    <!-- Custom -->
    <script src="{{assets}}/js/docs.js"></script>
    <script src="{{assets}}/js/mobile-nav.js"></script>
  </body>
</html>
